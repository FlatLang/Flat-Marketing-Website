{
  "name": "flat-marketing-website",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sst bind vite dev",
    "build:server": "airship --use-links install -f server -- -library -module",
    "build:vite": "vite build",
    "build": "npm run build:server && npm run build:vite",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev"
  },
  "devDependencies": {
    "@aws-sdk/client-ssm": "^3.354.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.2",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "aws-cdk-lib": "2.79.1",
    "constructs": "10.1.156",
    "dayjs": "^1.11.8",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "node-html-parser": "^6.1.5",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sst": "2.13.8",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.3",
    "svelte-kit-sst": "2.13.8",
    "svgo": "^3.0.2",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vitest": "^0.25.3"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.354.0",
    "@aws-sdk/lib-dynamodb": "^3.354.0"
  }
}